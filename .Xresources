! URxvt Settings

!URxvt.loginShell: true

URxvt.scrollBar: off
URxvt.visualBell: True

!URxvt.font: xft:Inconsolata-g:size=10
!URxvt.font: xft:Inconsolata:size=10
URxvt.font: xft:Terminus:antialias=false:size=8
!URxvt.font: xft:Deja Vu Sans Mono:size=8

!URxvt.fading: 25

! pseudo-transparency
URxvt.shading: 10
URxvt.transparent: true

! actual transparency, requires compositing wm
!URxvt.depth: 32
!URxvt.background: rgba:0000/0000/0000/d000

