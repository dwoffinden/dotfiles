! URxvt Settings

URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

!URxvt.loginShell: true

URxvt.scrollBar: off
URxvt.visualBell: True

!URxvt.font: xft:Inconsolata:size=10
!URxvt.font: xft:Terminus:antialias=false:size=8
!URxvt.font: xft:Deja Vu Sans Mono:size=9
URxvt.font: xft:Source Code Pro:size=9,xft:Deja Vu Sans Mono:size=9

URxvt.letterSpace: -2

URxvt.fading: 0

! actual transparency, requires compositor
URxvt.depth: 32
URxvt.background: [95]#002b36

#include ~/.config/base16-xresources/base16-solarized.dark.xresources


