! URxvt Settings

! make Home/End match other terminals
!URxvt.keysym.Home: \033[1~
!URxvt.keysym.End: \033[4~
!URxvt.keysym.KP_Home: \033[1~
!URxvt.keysym.KP_End:  \033[4~

URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.iso14755: False

!URxvt.loginShell: true

URxvt.scrollBar: off
URxvt.visualBell: True

!URxvt.font: xft:Inconsolata:size=10
!URxvt.font: xft:Terminus:antialias=false:size=8
!URxvt.font: xft:Deja Vu Sans Mono:size=9
URxvt.font: xft:Source Code Pro:size=9,xft:Deja Vu Sans Mono:size=9

URxvt.letterSpace: -2

URxvt.fading: 0

! actual transparency, requires compositor
URxvt.depth: 32
URxvt.background: [95]#002b36

! Bring gitk into the 90s
*TkTheme: clam

#include ".config/base16-xresources/xresources/base16-solarized-dark-256.Xresources"

